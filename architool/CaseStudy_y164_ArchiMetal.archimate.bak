      <folder name="HQ" id="id-38933bda264b45c1a6d4f2568b973a29">
        <element xsi:type="archimate:BusinessActor" name="QualityMgt" id="id-990fd8b2661143e9a8a62505f03eea7f"/>
        <element xsi:type="archimate:BusinessActor" name="Finance" id="id-22b2ad92374b4e18935540861e8c081e">
          <documentation>defines policies and procedures and rules that the DCs must follow when they set prices and assess the credit risk associated with individual customers.</documentation>
        </element>
        <element xsi:type="archimate:BusinessActor" name="HR" id="id-e6cd28550e744460a6082147f79e872a"/>
      </folder>
      <folder name="PC" id="id-142c4ed6e5f1434a9b7cbf4844e198e4">
        <element xsi:type="archimate:BusinessActor" name="Production" id="id-8f400676301645f5a62416182eaf86f1"/>
