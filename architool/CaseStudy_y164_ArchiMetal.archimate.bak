      <folder name="DC" id="id-13e77c73dfe04c6aad52acdbdf01a063">
        <element xsi:type="archimate:BusinessActor" name="Distribution" id="id-584f85cb84624ad0a4eba7eb187d5818">
          <documentation>in charge of transporting the finished goods to the customer</documentation>
        </element>
        <element xsi:type="archimate:BusinessActor" name="Commercial" id="id-ec69d13ff49b4d92a6fd71bbc599cb70"/>
        <element xsi:type="archimate:BusinessActor" name="Customer Relations" id="id-61d96b8063494859b45b6cdf588e0f56">
          <documentation>assesses credit risk, works with sales representatives to negotiate contracts, leads responses to customer compliants, ensures collection of accounts payable, and manages customer master data.</documentation>
        </element>
      </folder>
      <element xsi:type="archimate:BusinessActor" name="ArchiMetal" id="id-cf0e48b2ff60495a8ae8a01b7d9621a0"/>
      <element xsi:type="archimate:BusinessActor" name="HQ (Luxembourg)" id="id-f7027ec27744421c899bcbe43dea8436">
        <documentation>houses top management, as well as finance, quality management, product development, and human resources (HR) departments</documentation>
      </element>
      <element xsi:type="archimate:BusinessActor" name="Production Center (Poland)" id="id-a175819b897c4963b3db1bdee7df6317">
        <documentation>procures raw materials and uses them to manufacture products, which it stores in warehouses and distributes to DCs.</documentation>
      </element>
      <element xsi:type="archimate:BusinessActor" name="DC Benelux (Belgium)" id="id-3de5371b796a4ccd8cbca8a02bcf2805">
        <documentation>Serve the company's construction sector customers</documentation>
      </element>
      <element xsi:type="archimate:BusinessActor" name="DC Spain" id="id-5ba9feed70ee4281bad294c82298efd4">
        <documentation>Serve the company's construction sector customers</documentation>
      </element>
      <element xsi:type="archimate:BusinessActor" name="DC East Europe (Romania)" id="id-3171246ddbe74e61a1e19771fb947fc7">
        <documentation>covers other sectors customers</documentation>
      </element>
